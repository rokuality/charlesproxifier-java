<suite name="Rokuality Test Suite" parallel="methods" preserve-order="true" thread-count="1">

    <parameter name="ParameterToAdd" value="false"></parameter> <!-- true, false -->

    <listeners>
        
    </listeners>

    <test verbose="1" name="All Example Tests" annotations="JDK">
        <groups>
            <run>
                <include name="Debug" />
                <exclude name="Broken" />
                <exclude name="Sensitive" />
            </run>
        </groups>

        <packages>
            <package name="*" />
        </packages>
        
    </test>

</suite>
